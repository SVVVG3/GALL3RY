{
  "version": 2,
  "public": true,
  "framework": "create-react-app",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*\\.(js|json|css|ico|png|jpg|svg))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 