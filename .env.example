# Environment variables for the NFT Gallery app

# Alchemy API keys (for NFT images)
REACT_APP_ALCHEMY_ETH_API_KEY=your_alchemy_eth_api_key_here
REACT_APP_ALCHEMY_BASE_API_KEY=your_alchemy_base_api_key_here

# Zapper API key (for NFT data)
ZAPPER_API_KEY=your_zapper_api_key_here

# Neynar API key (alternative Farcaster API access)
REACT_APP_NEYNAR_API_KEY=your_neynar_api_key_here

# Farcaster Auth configuration
REACT_APP_FARCASTER_DOMAIN=nft-gallery.app
REACT_APP_FARCASTER_SIWE_URI=https://nft-gallery.app/login
REACT_APP_OPTIMISM_RPC_URL=https://mainnet.optimism.io

# MongoDB connection string (for production)
MONGODB_URI=your_mongodb_connection_string_here

# Server port (defaults to 3001)
PORT=3001 